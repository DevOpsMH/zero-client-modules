{
  "name": "zero-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@reach/router": "1.3.4",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "jquery": "3.5.1",
    "material-ui": "0.20.2",
    "promise-polyfill": "6.1.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-material-ui-form-validator": "2.1.1",
    "react-scripts": "4.0.0",
    "typescript": "4.1.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "init:all": "npm run init:hooks & npm run init:node_modules",
    "init:node_modules": "rimraf node_modules & unzip node_modules",
    "init:hooks": "ncp ./hooks ./.git/hooks"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "react-hooks/exhaustive-deps": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.6",
    "@types/reach__router": "1.3.6",
    "@types/react": "16.9.55",
    "@types/react-dom": "16.9.9",
    "@types/react-material-ui-form-validator": "2.1.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "2.1.2",
    "react-app-rewire-alias": "0.1.6",
    "react-app-rewired": "2.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
